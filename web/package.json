  "scripts": {
    "start": "bash ./src/scripts/dev.sh  && react-app-rewired start",
    "start:emulators": "bash ./src/scripts/ci.sh  && react-app-rewired start",
    "dev-script": "bash ./src/scripts/dev.sh",
    "build": "CI='' react-app-rewired build",
    "build-storybook": "build-storybook  -c .storybook -o build_story -s public",
    "test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "eslint:check": "eslint \"./src/**/*.{jsx,js,json,md,ts,tsx}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pre-commit-bck": "npm run prettier:fix && npm run eslint:check && npm test && npm run e2e",
    "pre-commit": "npm run prettier:fix && npm run eslint:check",
    "prettier:check": "prettier --check \"./src/**/*.{jsx,js,json,md,ts,tsx}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{jsx,js,json,md,ts,tsx}\"",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./src",
    "predeploy-webapp": "npm run build && rm -rf build_webapp && mv build build_webapp",
    "predeploy-story": "npm run build-storybook",
    "deploy-dev-webapp": "bash ./src/scripts/dev.sh && npm run predeploy-webapp && firebase deploy --only hosting:dev",
    "deploy-dev-story": "npm run predeploy-story && firebase deploy --only hosting:story",
    "deploy-dev": "npm run deploy-dev-webapp && npm run deploy-dev-story && firebase deploy --only hosting",
    "predeploy-dev": "firebase use dev",
    "deploy-dev:ci": "cross-env NODE_ENV=staging firebase deploy --only hosting:dev,hosting:story,firestore:indexes,firestore:rules --token $FIREBASE_TOKEN --non-interactive",
    "predeploy-prod": "firebase use production",
    "deploy-prod:ci": "cross-env NODE_ENV=production firebase deploy --only hosting:prod,firestore:indexes,firestore:rules --token $FIREBASE_TOKEN --non-interactive",
    "snapshot:ci": "react-scripts test --watchAll=false storybook.test.ts",
    "snapshot": "react-scripts test storybook.test.ts",
    "start-storybook": "start-storybook --ci --quiet -p 9009 -s public",
    "test:snapshot:ci": "start-server-and-test start-storybook http://localhost:9009 snapshot:ci",
    "test:snapshot": "start-server-and-test start-storybook http://localhost:9009 snapshot",
    "emulate": "firebase emulators:start --import=./firebase --only firestore,auth",
    "start:emulate": "firebase emulators:exec --import=./firebase --only firestore,auth 'npm run start:emulators'",
    "start:emulate:token": "firebase emulators:exec --project money-college-dev-8a907 --import=./firebase --only firestore,auth --token $FIREBASE_TOKEN 'npm run start:emulators'",
    "firebase": "firebase"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-storyshots": "^6.1.4",
    "@storybook/addon-storyshots-puppeteer": "^6.1.4",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.18",
